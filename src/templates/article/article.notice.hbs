{{set root='..'}}
{{> header cssName='article.notice'}}
	<div class="main-container">
		<div class="tab-p-snbr">
			{{> login}}
			<div class="sub-banner-notice">
				<div class="notice-menu">
					<img src="{{root}}/img/tit_news.jpg">
					<ul>
						<li class="active">공지사항</li>
						<li>업데이트</li>
						<li>이클립스</li>
						<li>매거진</li>
						<li>이벤트</li>
					</ul>
				</div>
				{{> sub.banner.img}}
			</div>
		</div>
		<div class="tit-new-notice">
			<div class="notice-title-img">
				<img src="{{root}}/img/tit_news_notice.jpg" alt="공지사항">
			</div>
			<div class="post-content">
				<div class="search-notice">
					<select id="search-news-or-notice" >
						<option value="title">제목</option>
						<option value="title+contents">제목 + 내용</option>
					</select>
					<input type="text" class="input-search-notice" style="width: 160px; height: 21px;">
					<input type="button" class="notice-search-btn" style="font-size: 11px; background-color: black; color: white;" value="검색">
				</div>
				<table class="table-article" width="100%" cellpadding="0" cellspacing="0">
					<colgroup>
						<col width="80%">
						<col width="10%">
						<col width="10%">
					</colgroup>
					<thead>
						<tr>
							<th col="scope">제목</th>
							<th col="scope">날짜</th>
							<th col="scope" class="col-right">조회</th>
						</tr>
					</thead>
					<tbody></tbody>
				</table>
				<div class="write-notice-btn">
					<img src="{{root}}/img/btn-wrt.jpg">
				</div>
				<div class="paging">
					<ul>
						<li><img src="{{root}}/img/paging_pre2.jpg"> </li>
						<li class="active">1</li>
						<li>2</li>
						<li>3</li>
						<li>4</li>
						<li>5</li>
						<li>6</li>
						<li>7</li>
						<li>8</li>
						<li>9</li>
						<li cl>10</li>
						<li><img src="{{root}}/img/paging_next2.jpg"> </li>
					</ul>
				</div>
			</div>
		</div>

{{> footer jsName='article.notice'}}
